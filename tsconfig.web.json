{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/preload/*.d.ts",
    "src/renderer/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@utils/*": [
        "./src/renderer/utils/*"
      ],
      "@assets/*": [
        "./src/renderer/assets/*"
      ],
      "@components/*": [
        "./src/renderer/components/*"
      ],
      "@store/*": [
        "src/renderer/store/*"
      ],
      "@localTypes/*": [
        "src/renderer/types/*"
      ],
    }
  }
}